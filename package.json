    "i18next": "^23.16.4",
    "immer": "^10.1.1",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-contexify": "^6.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.27.0",
